<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageId>CodeChavez.Repository.Pattern</PackageId>
		<Title>CodeChavez Repository Pattern Library</Title>
		<Authors>Victor A Chavez</Authors>
		<Product>CodeChavez.Repository.Pattern</Product>
		<Version>9.0.0</Version>
		<Copyright>2020-2025</Copyright>
		<Description>Has some useful extension and utilities that allow for every day development</Description>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageProjectUrl>https://github.com/codechavez/codechavez.repository.pattern</PackageProjectUrl>
		<RepositoryUrl>https://github.com/codechavez/codechavez.repository.pattern</RepositoryUrl>
		<PackageLicenseFile></PackageLicenseFile>
		<PackageTags>CodeChavez,Repository,Pattern,Common,Helpers,Utilities,.net</PackageTags>
	</PropertyGroup>
	
	<ItemGroup>
		<None Include="..\..\README.md" Pack="true" PackagePath="\" />
		<None Include="..\..\LICENSE" Pack="true" PackagePath="\" />
	</ItemGroup>
	
	<ItemGroup>
		<PackageReference Include="CodeChavez.Common" Version="9.0.1" />
		<PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.9" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.InMemory" Version="9.0.9" />
		<PackageReference Include="MongoDB.Bson" Version="3.5.0" />
	</ItemGroup>

</Project>
